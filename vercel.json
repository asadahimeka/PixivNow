{
  "github": {
    "silent": true
  },
  "functions": {
    "api/*.ts": {
      "memory": 128
    }
  },
  "rewrites": [
    {
      "source": "/:__PREFIX(ajax|rpc|.+\\.php)/:__PATH*",
      "destination": "/api/http"
    }
  ],
  "redirects": [
    {
      "source": "/-/:__PATH*",
      "destination": "https://nfn.kanata.ml/pximg/:__PATH"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
