{
  "github": {
    "silent": true
  },
  "functions": {
    "api/*.ts": {
      "memory": 128
    }
  },
  "rewrites": [
    {
      "source": "/:__PREFIX(ajax|rpc|.+\\.php)/:__PATH*",
      "destination": "/api/http"
    }
  ],
  "redirects": [
    {
      "source": "/-/:__PATH*",
      "destination": "https://kw-nf.pixiv.pics/pximg/:__PATH"
    },
    {
      "source": "/~/:match*",
      "destination": "https://s.pximg.net/:match"
    }
  ]
}
