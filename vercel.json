{
  "github": {
    "silent": true
  },
  "functions": {
    "api/*.ts": {
      "memory": 128
    }
  },
  "rewrites": [
    {
      "source": "/:__PREFIX(ajax|rpc|.+\\.php)/:__PATH*",
      "destination": "/api/http"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/-/:__PATH*",
      "destination": "https://nfn.kanata.ml/pximg/:__PATH"
    }
  ],
  "headers": []
}
