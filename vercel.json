{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/:__PREFIX(~)/:__PATH(.+)",
      "destination": "/api/image"
    },
    {
      "source": "/:__PREFIX(ajax|rpc|.+\\.php)/:__PATH*",
      "destination": "/api/http"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/-/:__PATH*",
      "destination": "https://pximg.cocomi.cf/:__PATH"
    }
  ],
  "headers": []
}
